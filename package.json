{
  "name": "redux-friends",
  "version": "0.0.4",
  "description": "a set of helpers / builders for redux",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -d dist --copy-files",
    "patch": "versionifier patch",
    "minor": "versionifier minor",
    "major": "versionifier major",
