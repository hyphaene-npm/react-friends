    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyphaene/redux-friends.git"
  },
  "keywords": [
    "redux",
    "helpers",
    "builders"
  ],
  "author": "maximilien garenne <maximilien.garenne@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hyphaene/redux-friends/issues"
  },
  "homepage": "https://github.com/hyphaene/redux-friends#readme",
  "dependencies": {
    "redux-actions": "^2.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "versionifier": "^1.3.4"
  },
  "versionifier": {
    "commitIfOnlyPackageJsonInStage": false,
    "commitIfMultipleFilesInStage": true,
    "push": true,
    "commitMessage": "Updated package.json to version @@",
    "remoteRepo": "origin",
    "publish": true
